#pragma once

#if defined(__linux__)
#  if defined(__x86_64__)
#    include <uint32_t>
//   @see include/uapi/asm-generic/posix_types.h
     typedef /* unsigned int */ uint32_t gid_t;
#  else
#    error
#  endif
#elif defined(__FreeBSD__)
#  include <uint32_t>
// @see sys/sys/_types.h
   typedef uint32_t gid_t;
#else
#  error
#endif
